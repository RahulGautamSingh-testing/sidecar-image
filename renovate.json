{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "regexManagers": [
    {
      "fileMatch": ["config/options/index.ts"],
      "matchStrings": [
        "default: 'ghcr.io/containerbase/sidecar:(?<currentValue>.*)'"
      ],
      "depNameTemplate": "containerbase/sidecar",
      "datasourceTemplate": "docker"
    }
  ]
}
